{
    "admin": {
        "export": {
            "default": {
                "properties": [
                    "search_ident",
                    "ts",
                    "keyword",
                    "num_results"
                ],
                "orders": [
                    {
                        "property": "ts",
                        "mode": "desc"
                    }
                ],
                "exporter_options": {
                    "convert_br_to_newlines": true,
                    "strip_tags": true,
                    "filename": "search-logs.csv"
                }
            }
        },
        "default_export": "default",
        "lists": {
            "default": {
                "properties": [
                    "search_ident",
                    "ts",
                    "keyword",
                    "num_results"
                ],
                "orders": [
                    {
                        "property": "ts",
                        "mode": "desc"
                    }
                ],
                "list_actions": {
                    "export": {
                        "url": "object/export?obj_type=charcoal/search/search-log",
                        "label": "Export"
                    }
                }
            }
        },
        "default_list": "default",

        "forms": {
            "default": {}
        },
        "default_form": "default",

        "dashboards": {
            "admin.edit": {},
            "admin.table": {
                "title": {
                    "fr": "Recherches",
                    "en": "Searches"
                },
                "layout": {
                    "structure": [
                        { "columns": [ 1, 1 ] },
                        { "columns": [ 1 ] }
                    ]
                },
                "widgets": {
                    "top": {
                        "type": "charcoal/admin/widget/search/top-search",
                        "priority": 1
                    },
                    "no-results": {
                        "type": "charcoal/admin/widget/search/no-results-search",
                        "priority": 2
                    },
                    "table": {
                        "type": "charcoal/admin/widget/table",
                        "collection_ident": "default",
                        "obj_type": "charcoal/search/search-log",
                        "priority": 3,
                        "collection_config": {
                            "orders": [
                                {
                                    "property": "ts",
                                    "mode": "desc"
                                }
                            ],
                            "page": 1,
                            "num_per_page": 40,
                            "list_actions": {
                                "export": {
                                    "url": "object/export?obj_type=charcoal/search/search-log",
                                    "label": "Export"
                                }
                            }
                        }
                    }
                }
            }
        },
        "default_create_dashboard": "admin.edit",
        "default_edit_dashboard": "admin.edit",
        "default_collection_dashboard": "admin.table"
    }
}
