{
    "admin": {
        "export": {
            "default": {
                "properties": [
                    "searchIdent",
                    "ts",
                    "keyword",
                    "numResults"
                ],
                "orders": [
                    {
                        "property": "ts",
                        "mode": "desc"
                    }
                ],
                "exporterOptions": {
                    "convertBrToNewlines": true,
                    "stripTags": true,
                    "filename": "search-logs.csv"
                }
            }
        },

        "defaultExport": "default",

        "lists": {
            "default": {
                "properties": [
                    "searchIdent",
                    "ts",
                    "keyword",
                    "numResults"
                ],
                "orders": [
                    {
                        "property": "ts",
                        "mode": "desc"
                    }
                ],
                "listActions": {
                    "export": {
                        "url": "object/export?obj_type=charcoal/search/search-log",
                        "label": "Export"
                    }
                }
            }
        },

        "defaultList": "default",

        "forms": {
            "default": {}
        },

        "defaultForm": "default",

        "dashboards": {
            "admin.edit": {},
            "admin.table": {
                "title": {
                    "fr": "Recherches",
                    "en": "Searches"
                },
                "layout": {
                    "structure": [
                        { "columns": [ 1, 1 ] },
                        { "columns": [ 1 ] }
                    ]
                },
                "widgets": {
                    "top": {
                        "type": "charcoal/admin/widget/search/top-search",
                        "priority": 1
                    },
                    "no-results": {
                        "type": "charcoal/admin/widget/search/no-results-search",
                        "priority": 2
                    },
                    "table": {
                        "type": "charcoal/admin/widget/table",
                        "collectionIdent": "default",
                        "objType": "charcoal/search/search-log",
                        "priority": 3,
                        "collectionConfig": {
                            "orders": [
                                {
                                    "property": "ts",
                                    "mode": "desc"
                                }
                            ],
                            "page": 1,
                            "numPerPage": 40,
                            "listActions": {
                                "export": {
                                    "url": "object/export?obj_type=charcoal/search/search-log",
                                    "label": "Export"
                                }
                            }
                        }
                    }
                }
            }
        },

        "defaultCreateDashboard": "admin.edit",
        "defaultEditDashboard": "admin.edit",
        "defaultCollectionDashboard": "admin.table"
    }
}
